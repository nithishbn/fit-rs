{
  "$schema": "https://charm.land/crush.json",
  "options": {
    "debug": true
  },
  "providers": {
    "OpenRouter extended": {
      "type": "openai",
      "base_url": "https://openrouter.ai/api/v1",
      "api_key": "$OPENROUTER_API_KEY",
      "extra_body": {
        "provider": {
          "order": ["cerebras/fp8"],
          "allow_fallbacks": false
        }
      },
      "models": [
        {
          "id": "qwen/qwen3-coder",
          "name": "Qwen3 Coder",
          "cost_per_1m_in": 0.2,
          "cost_per_1m_out": 0.8,
          "cost_per_1m_in_cached": 0.2,
          "cost_per_1m_out_cached": 0.8,
          "context_window": 262144,
          "default_max_tokens": 260000
        }
      ]
    }
  }
}
