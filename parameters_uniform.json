{
  "input": {
    "file": "data/data.csv",
    "output_dir": "ec50_output_uniform"
  },
  "mcmc": {
    "samples": 300,
    "burnin": 150,
    "chains": 3,
    "sigma": 0.05,
    "backend": "stan"
  },
  "priors": {
    "emin": {
      "type": "uniform",
      "mean": 0.0,
      "std": 0.0,
      "min": -20.0,
      "max": 20.0,
      "description": "Lower asymptote with uniform prior"
    },
    "emax": {
      "type": "uniform", 
      "mean": 0.0,
      "std": 0.0,
      "min": 50.0,
      "max": 150.0,
      "description": "Upper asymptote with uniform prior"
    },
    "ec50": {
      "type": "normal",
      "mean": -6.0,
      "std": 2.0,
      "description": "EC50 with normal prior (log10 scale)"
    },
    "hillslope": {
      "type": "lognormal",
      "mean": 0.0,
      "std": 0.5,
      "description": "Hill slope with log-normal prior"
    }
  },
  "plotting": {
    "bounds": {
      "x_min": 1e-9,
      "x_max": 1e-3,
      "y_min": null,
      "y_max": null
    },
    "verbose": true
  },
  "metadata": {
    "description": "EC50 fitting with mixed priors (uniform + normal + log-normal)",
    "version": "1.0",
    "created": "2024-01-01T00:00:00Z"
  }
}